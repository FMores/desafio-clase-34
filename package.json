		"prod:ecosystem2": "npm run build && pm2 start ./dist/config/ecosystem2.config.js",
		"artillery:fork": "artillery quick --count 50 -n 20 http://localhost:8080/api/system/info",
		"artillery:cluster": "artillery quick --count 50 -n 20 http://localhost:8080/api/system/info",
		"0x": "0x ./dist",
		"migrate:db": "npx knex migrate:latest --env development",
		"rollback:db": "npx knex migrate:rollback --env development",
		"seed:db": "npx knex seed:run --env development"
	},
